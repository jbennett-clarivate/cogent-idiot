{
  "name": "cogent-idiot",
  "version": "1.0.0",
  "description": "Angular v20 application with Node.js backend - migrated from legacy HTML/JS",
  "main": "server.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve cogent-idiot",
    "build:dev": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "server": "node --no-deprecation server.js",
    "dev": "concurrently \"node --no-deprecation server.js\" \"ng serve\"",
    "build:prod": "ng build --configuration production"
  },
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "mysql2": "^3.14.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.7.0",
    "concurrently": "^7.6.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-coverage": "~2.2.0",
    "karma-firefox-launcher": "^2.1.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.0"
  },
  "private": true
}
